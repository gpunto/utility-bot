CREATE TABLE IF NOT EXISTS counter (
  name TEXT NOT NULL,
  date INTEGER NOT NULL,
  ownerId INTEGER NOT NULL,
  chatId INTEGER NOT NULL,

  PRIMARY KEY (name, ownerId)
);

insert:
INSERT INTO counter (name, date, ownerId, chatId)
VALUES (?, ?, ?, ?);

selectAll:
SELECT *
FROM counter;

selectOne:
SELECT *
FROM counter
WHERE name = ? AND ownerId = ?;
